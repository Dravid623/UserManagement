<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>">
            <span>Update</span>
        </a>
        <a class="delete" data-id=<%= users[i]._id%> >
            <span>Delete</span>
        </a>
    </td>
</tr>
<% } %>